{
	"name": "madagascar-bot",
	"version": "1.0.0",
	"main": "index.js",
	"repository": "git@github.com:TheMartonfi/madagascar-bot.git",
	"author": "TheMartonfi <carlmartonfi@hotmail.com>",
	"license": "MIT",
	"scripts": {
		"tsc:w": "tsc -w",
		"postinstall": "tsc",
		"start": "node build/index.js",
		"start:dev": "concurrently \"yarn tsc:w\" \"nodemon build/index.js\"",
		"db:migrate": "DB_MIGRATE=true node build/index.js",
		"db:seed": "DB_SEED=true node build/index.js",
		"db:reset": "DB_MIGRATE=true DB_SEED=true node build/index.js"
	},
	"dependencies": {
		"typescript": "^4.1.3",
		"@typeit/discord": "^4.0.8",
		"discord.js": "^12.5.1",
		"dotenv": "^8.2.0",
		"pg": "^8.5.1",
		"sequelize": "^6.3.5"
	},
	"devDependencies": {
		"@types/node": "^14.14.21",
		"@types/pg": "^7.14.9",
		"@types/validator": "^13.1.3",
		"concurrently": "^5.3.0",
		"nodemon": "^2.0.7"
	},
	"engines": {
		"node": "14.15.3"
	}
}
